<tr>
  <td><%= test.title %></td>
  <td style="text-align: left"><%= test_level(test) %></td>
  <td style="text-align: center"><%= test.questions.count %></td>
  <td><%= link_to 'Show', test_path(test) %></td>
  <td><%= link_to 'Edit', edit_test_path(test) %></td>
  <td>
    <%= link_to 'Delete',
    test_path(test),
    method: :delete,
    data: { confirm: 'Are you sure?'} %>
  </td>
</tr>

<tr>
  <td><%= answer.body %></td>
  <td><%= answer.correct %></td>
  <td><%= link_to admin_answer_path(answer) do %>
        <i class="fa fa-table" aria-hidden="true"></i>
      <% end %>
  </td>
  <td><%= link_to edit_admin_answer_path(answer) do %>
        <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
      <% end %>
  </td>
  <td>
      <%= link_to admin_answer_path(answer),method: :delete, data: { confirm: 'Are you sure?'} do %>
        <i class="fa fa-trash-o" aria-hidden="true"></i>
      <% end %>
  </td>
</tr>

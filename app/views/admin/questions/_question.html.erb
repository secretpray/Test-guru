<tr>
  <td><%= question.body %></td>
  <td style="text-align: center"><%= question.answers.count %></td>
  <td><%= link_to edit_admin_question_path(question) do %>
     <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
      <% end %>
  </td>
  <td><%= link_to admin_question_path(question), method: :delete, data: { confirm: 'Are you sure?'} do %>
        <i class="fa fa-trash-o" aria-hidden="true"></i>
      <% end %>
  </td>
  <td><%= link_to admin_question_path(question) do%>
        <i class="fa fa-table" aria-hidden="true"></i>
      <% end %>
  </td>
</tr>
